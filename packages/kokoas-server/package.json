{
  "name": "kokoas-server",
  "version": "1.0.0",
  "description": "Kokoas server",
  "main": "./dist/kokoas-server/src/main.js",
  "scripts": {
    "test": "jest",
    "dev": "nodemon",
    "tsc": "tsc src/main.ts --outDir ./dist --esModuleInterop --skipLibCheck",
    "dev:local": "cross-env NODE_ENV=local nodemon src/main.ts",
    "dev:staging": "cross-env NODE_ENV=staging nodemon src/main.ts",
    "upstart": "cross-env NODE_ENV=prod git pull && npm run start",
    "build": "cross-env NODE_ENV=prod rimraf ./dist && tsc --build tsconfig.json",
    "start": "cross-env NODE_ENV=prod ts-node --files ./src/main.ts -T",
    "forever": "forever start --uid kokoas-server -a -v -o kokoas-server.log -e kokoas-server-err.log -c npm start",
    "dtsgen:projDetails": "npx @kintone/dts-gen --base-url https://rdmuhwtt6gx7.cybozu.com/ --app-id 194 --type-name Data --namespace ProjectDetails -o src/types/data.projectDetails.d.ts",
    "dtsgen:projEstimates": "npx @kintone/dts-gen --base-url https://rdmuhwtt6gx7.cybozu.com/ --app-id 202 --type-name Data --namespace ProjectEstimates -o src/types/data.projectEstimates.d.ts",
    "dtsgen:custgroups": "npx @kintone/dts-gen --base-url https://rdmuhwtt6gx7.cybozu.com/ --app-id 185 --type-name Data --namespace CustomerGroupTypes -o src/types/data.customerGroup.d.ts",
    "dtsgen:customers": "npx @kintone/dts-gen --base-url https://rdmuhwtt6gx7.cybozu.com/ --app-id 173 --type-name Data --namespace CustomerTypes -o src/types/data.customers.d.ts",
    "dtsgen:employees": "npx @kintone/dts-gen --base-url https://rdmuhwtt6gx7.cybozu.com/ --app-id 34 --type-name Data --namespace Employees -o src/types/data.employees.d.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/rimraf": "^3.0.2",
    "@types/rmfr": "^2.0.1",
    "@types/user-agents": "^1.0.2",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8"
  },
  "dependencies": {
    "@pdf-lib/fontkit": "^1.1.1",
    "babel-jest": "^27.5.1",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^7.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.1",
    "libs": "*",
    "node-cron": "^3.0.0",
    "pdf-img-convert": "^1.0.6",
    "pdf-lib": "^1.17.1",
    "pdf2pic": "^2.1.4",
    "rmfr": "^2.0.0",
    "types": "*",
    "uuid": "^9.0.0",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
